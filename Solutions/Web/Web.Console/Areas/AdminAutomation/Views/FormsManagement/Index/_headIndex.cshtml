@using VM.Public;
@using Newtonsoft.Json

<script type="text/javascript">
    @{

        List<FormUsagesVM> formUsagesList = new List<FormUsagesVM>();

        @Html.Raw("var formUsagesList = [];")
        ;

        if (ViewData["FormUsagesList"] != null)
        {
            /*List<DomainsSettingsVM> */
            formUsagesList = (List<FormUsagesVM>)ViewData["FormUsagesList"];

            var formUsagesListData = formUsagesList.Select(c =>
            new
            {
                DisplayText = $"{c.FormUsageTitle}",
                Value = c.FormUsageId
            });

            @Html.Raw("debugger; formUsagesList = " +JsonConvert.DeserializeObject(JsonConvert.SerializeObject(formUsagesListData)) + "; debugger;")
            ;



        }

    }
</script>

