                    debugger;
                    if (result.Result == "ReturnName") {

                        $('#listContainer').jtable('reload');
                        $('#my-warning-modal').find('.modal-title').html('اخطار');
                        $('#my-warning-modal').find('#error').html("یک پروژه با نام " + result.Record + " در حال حاضر تیک نمایش در داشبورد را دارد");
                        $('#my-warning-modal').modal("show");
                    }
                    else
                        if (result.Result == "ERROR") {
                            $('#my-warning-modal').find('.modal-title').html('خطا');
                            $('#my-warning-modal').find('#error').html('خطا');
                            $('#my-warning-modal').modal("show");
                        }
                        else {
                            $('#listContainer').jtable('reload');
                        }
                }

            });
        });



