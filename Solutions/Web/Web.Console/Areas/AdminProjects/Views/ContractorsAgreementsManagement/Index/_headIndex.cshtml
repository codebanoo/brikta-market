<script type="text/javascript">
    @{
        long constructionId = 0;
                @Html.Raw("var constructionProjectId = 0;")
        ;

        if (ViewData["ConstructionProjectId"] != null)
        {
            constructionId = (long)ViewData["ConstructionProjectId"];


                    @Html.Raw("debugger; constructionProjectId =" + constructionId.ToString() +" ;debugger;")
            ;
        }
    }

</script>
<script type="text/javascript">
    @{
        string domainName = "";

                        @Html.Raw("var domainName;")
        ;

        if (ViewData["DomainName"] != null)
        {
            domainName = ViewData["DomainName"] as string;

                            @Html.Raw(" domainName = '" + domainName + "'; ")
            ;
        }
    }
</script>

