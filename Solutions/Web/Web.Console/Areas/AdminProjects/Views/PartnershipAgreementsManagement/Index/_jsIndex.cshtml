        //#region Function For Get Conversation
        function getConversation(recordId, contractType) {
            debugger;

            $('.chat').remove();

            $('.chat chat-left').html('');
            $('.modal-footer').html('');
            $('.text-slate-500').html('');

            debugger;
            $.ajax({
                cache: false,
                type: "POST",
                url: "/AdminProjects/PartnershipAgreementsManagement/GetConversationDataByAgreementTypeAndRecordId",
                data: { 'recordId': recordId, 'contractType': contractType },
                success: function (result) {
                    if (result === null || result === '' || result === undefined || result.length <= 0) {
                        $('#my-warning-modal').find('.modal-title').html('خطا');
                        $('#my-warning-modal').find('#error').html('خطا');
                        return $('#warningModal').click();
                    }

                    let contractsConversations = '';
                    if (result.Records.length > 0) {
                        debugger;
                        $('.ChatHeader').append(' - ' + result.Records[0].RecordTitle);
                        for (var j = 0; j < result.Records.length; j++) {
                            if (result.Records[j].UserIdCreator == userId) {   // چت خود فرد
                                debugger;
                                var name = result.Records[j].CustomUsersVM.Name;
                                var family = result.Records[j].CustomUsersVM.Family;
                                var description = result.Records[j].ConversationLogDescription.replace(/\r\n|\n/g, "<br>");
                                contractsConversations +=
                                    '<div class="chat">' +
                                    '<div class="chat-avatar">' +
                                    '<a class="avatar avatar-online" data-placement="right">' +
                                    '<img alt="Midone - HTML Admin Template" class="rounded-full" src="/images/profileImage.png">' +
                                    '<i></i>' +
                                    '</a>' +
                                    '</div>' +
                                    '<div class="chat-body">' +
                                    '<div class="chat-content">' +
                                    '<p class="name">' + name + ' ' + family + '</p>' +
                                    '<p>' + 
                                    (description || "").replace(/(\r\n|\r|\n)/g, "<br>")
                                    + '</p>' +
                                    '<span>' + result.Records[j].OperationDateFa + '</span>' + ' ' + '<span>' + result.Records[j].OperationTime + '</span>' +
                                    '</div>' +
                                    '</div>' +
                                    '</div>';



                            } else { //چت طرف مقابل
                                var name = result.Records[j].CustomUsersVM.Name;
                                var family = result.Records[j].CustomUsersVM.Family;
                                var description = result.Records[j].ConversationLogDescription.replace(/\r\n|\n/g, "<br>");
                                contractsConversations +=
                                    '<div class="chat chat-left">' +
                                    '<div class="chat-avatar">' +
                                    '<a class="avatar avatar-online" data-toggle="tooltip" href="#" data-placement="left" title="" data-original-title="Edward Fletcher">' +
                                    '<img alt="Midone - HTML Admin Template" class="rounded-full" src="/images/profileImage.png">' +
                                    '<i></i>' +
                                    '</a>' +
                                    '</div>' +
                                    '<div class="chat-body">' +
                                    '<div class="chat-content">' +
                                    '<p class="other">' + name + ' ' + family + '</p>' +
                                    '<p>' +
                                    (description || "").replace(/(\r\n|\r|\n)/g, "<br>")
                                    + '</p>' +
                                    '<span>' + result.Records[j].OperationDateFa + '</span>' + ' ' + '<span>' + result.Records[j].OperationTime + '</span>' +
                                    '</div>' +
                                    '</div>' +
                                    '</div>';
                            }


                        }



                    } else {
                        debugger;
                        contractsConversations = '<div class="NullMessage" data-RecordId="' + recordId + '" style="text-align: center;margin-top:14rem;"><p class=" mt-5 h-48 font-medium"></div><div class="text-slate-500 mt-1" style="font-size:medium !important; text-align:center">در حال حاضر هیچ پیامی ندارید</p></div>';


                    }


                    //textArea for type the message
                    var typeChat =
                        '<textarea data-agreementid="' + recordId + '"  class="messageClass TextArea chat__box__input form-control dark:bg-darkmode-600 h resize-none border-transparent px-5 py-3 shadow-none focus:border-transparent focus:ring-0 height46" rows="1" placeholder="پیام خود را تایپ کنید..." data-contractType="PartnershipAgreement"></textarea>';


                    $('.modal-footer').append(typeChat);
                    $('.mainMessages').after(contractsConversations);
                    $('.Viewcount[data-agreementid="' + recordId + '"]').html('0');
                }

            });


        }


