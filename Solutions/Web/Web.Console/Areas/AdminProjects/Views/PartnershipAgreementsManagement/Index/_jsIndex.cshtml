        $('body').on('keydown', '.messageClass', function (event) {
            var _this = $(this);

            if (event.which === 13 && !event.shiftKey) {
                debugger;
                var formData = new FormData();
                var chatMessage = $('.messageClass').val();
                var contractType = _this.attr('data-contractType');
                var agreementId = _this.attr('data-AgreementId');


                formData.append('ConversationLogsVM.RecordId', agreementId);
                formData.append('ConversationLogsVM.TableTitle', contractType);
                formData.append('ConversationLogsVM.ConversationLogDescription', chatMessage);


                $.ajax({

                    cache: false,
                    type: "POST",
                    url: "/AdminProjects/PartnershipAgreementsManagement/AddToConversationLogs",
                    data: formData,
                    processData: false,
                    contentType: false,
                    success: function (result) {
                        if (result === null || result === '' || result === undefined || result.length <= 0) {
                            $('#my-warning-modal').find('.modal-title').html('خطا');
                            $('#my-warning-modal').find('#error').html('خطا');
                            return $('#warningModal')[0].click();
                        } else {
                            debugger;
                            var name = result.Record.CustomUsersVM.Name;
                            var family = result.Record.CustomUsersVM.Family;
                            var description = result.Record.ConversationLogDescription.replace(/\r\n|\n/g, "<br>");
                            var newChat =
                                '<div class="chat">' +
                                '<div class="chat-avatar">' +
                                '<a class="avatar avatar-online" data-toggle="tooltip" href="#" data-placement="right">' +
                                '<img alt="Midone - HTML Admin Template" class="rounded-full" src="/images/profileImage.png">' +
                                '<i></i>' +
                                '</a>' +
                                '</div>' +
                                '<div class="chat-body">' +
                                '<div class="chat-content">' +
                                '<p class="name">' + name + ' ' + family + '</p>' +
                                '<p>' +
                                (description || "").replace(/(\r\n|\r|\n)/g, "<br>")
                                + '</p>' +
                                '<span>' + result.Record.OperationDateFa + '</span>' + ' ' + '<span>' + result.Record.OperationTime + '</span>' +
                                '</div>' +
                                '</div>' +
                                '</div>';

                            if ($('.chat').length > 0) {

                                $(newChat).insertAfter($('.chat').last());
                            } else {

                                $('.NullMessage').remove();
                                $('.text-slate-500').html('');
                                $('.text-slate-500').remove();

                                $(newChat).insertAfter($('.mainMessages'));
                            }


                            $('.messageClass').val('');

                        }
                    }
                });

                debugger;
            }
        });


        //#endregion

    });


</script>