@using VM.Public;
@using VM.Teniaco;
@using Newtonsoft.Json;

<script type="text/javascript">
    @{
        AgenciesVM agenciesList = new AgenciesVM();

        @Html.Raw("var agenciesList;")
        ;

        if (ViewData["AgenciesList"] != null)
        {
            agenciesList = ViewData["AgenciesList"] as AgenciesVM;

            @Html.Raw("debugger; agenciesList = " +
        JsonConvert.DeserializeObject(JsonConvert.SerializeObject(agenciesList)) + "; debugger;")
            ;
        }
    }
</script>


