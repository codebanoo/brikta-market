@using VM.Public;
@using VM.Teniaco;
@using Newtonsoft.Json;

<script type="text/javascript">
    @{
        List<EvaluationSubjectsVM> SubjectsList = new List<EvaluationSubjectsVM>();
        @Html.Raw("var subjsList =[];")
        ;

        if (ViewData["Subjectlist"] != null)
        {
            SubjectsList = (List<EvaluationSubjectsVM>)ViewData["Subjectlist"];

            var ListData = SubjectsList.Select(a =>

            new
            {
                DisplayText = $"{a.Title}",
                Value = a.EvaluationSubjectId,
            });

            @Html.Raw(" subjsList =" +JsonConvert.DeserializeObject(JsonConvert.SerializeObject(ListData))+" ;")
            ;
        }
    }

</script>


@{
