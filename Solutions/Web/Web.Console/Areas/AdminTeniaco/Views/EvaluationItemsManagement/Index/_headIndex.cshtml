@using Newtonsoft.Json;
@using VM.Teniaco;


<script type="text/javascript">
    @{
        EvaluationQuestionsVM evaluationQuestion = new EvaluationQuestionsVM();
        @Html.Raw("var evaluationQuestion ;")
        ;

        if (ViewData["EvaluationQuestionsVM"] != null)
        {
            evaluationQuestion = (EvaluationQuestionsVM)ViewData["EvaluationQuestionsVM"];

            @Html.Raw("debugger; evaluationQuestion =" +JsonConvert.DeserializeObject(JsonConvert.SerializeObject(evaluationQuestion))+" ;debugger;")
            ;
        }
    }

</script>






@{
    await Html.RenderPartialAsync("jTableCssJs");
}


