@using VM.Public;
@using VM.Teniaco;
@using Newtonsoft.Json;

<script type="text/javascript">
    @{
        List<EvaluationSubjectsVM> evaluationSubjectsList = new List<EvaluationSubjectsVM>();
        @Html.Raw("var evaluationSubjectsList =[];")
        ;

        if (ViewData["EvaluationSubjectsList"] != null)
        {
            evaluationSubjectsList  = (List<EvaluationSubjectsVM>)ViewData["EvaluationSubjectsList"];

            var evaluationSubjectsListData = evaluationSubjectsList.Select(a =>

                new
                {
                    DisplayText = $"{a.Title}",
                    Value = a.EvaluationSubjectId
                });

            @Html.Raw("debugger; evaluationSubjectsList =" +JsonConvert.DeserializeObject(JsonConvert.SerializeObject(evaluationSubjectsListData))+" ;debugger;")
            ;
        }
    }

</script>



@{
    await Html.RenderPartialAsync("jTableCssJs");
}


