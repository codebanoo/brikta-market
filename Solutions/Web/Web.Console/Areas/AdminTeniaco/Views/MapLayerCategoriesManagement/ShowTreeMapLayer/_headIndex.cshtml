@using Newtonsoft.Json
@using VM.Teniaco
<script type="text/javascript">
    @{
        List<MapLayerCategoriesNodeWithDataVM> mapLayerCategoriesNodeWithDataVM = new List<MapLayerCategoriesNodeWithDataVM>();

        @Html.Raw("var jsonData;")
        ;
        

        if (ViewData["MapLayerCatList"] != null)
        {
            mapLayerCategoriesNodeWithDataVM = ViewData["MapLayerCatList"] as List<MapLayerCategoriesNodeWithDataVM>;

            @Html.Raw(" jsonData = " +
        JsonConvert.DeserializeObject(JsonConvert.SerializeObject(mapLayerCategoriesNodeWithDataVM)) + "; ")
        ;
        }
    }
</script>
