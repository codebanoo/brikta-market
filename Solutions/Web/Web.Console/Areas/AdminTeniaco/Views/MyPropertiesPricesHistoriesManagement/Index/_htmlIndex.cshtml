@using VM.Teniaco;


@{
    MyPropertiesVM property = new MyPropertiesVM();

    if (ViewData["MyPropertiesVM"] != null)
    {
        property = (MyPropertiesVM)ViewData["MyPropertiesVM"];

    }
}

<div id="searchBlock" class="col-12">
    <div class="row">
        <div class="col-md-3 col-xs-12 col-sm-12 col-lg-3 col-xl-3">
            <div>
                عنوان ملک:
                @Html.Raw(property.PropertyCodeName)
            </div>
        </div>
        <div class="col-md-3 col-xs-12 col-sm-12 col-lg-3  col-xl-3">
            <div>
                نام استان:
                @Html.Raw(property.MyPropertyAddressVM.StateName)
            </div>
        </div>
        <div class="col-md-3 col-xs-12 col-sm-12 col-lg-3  col-xl-3">
            <div>
                نام شهر:
                @Html.Raw(property.MyPropertyAddressVM.CityName)
            </div>
        </div>
        <div class="col-md-3 col-xs-12 col-sm-12 col-lg-3  col-xl-3">
            <div>
                نام منطقه:
                @Html.Raw(property.MyPropertyAddressVM.ZoneName)
            </div>
        </div>
    </div>
</div>
<hr class="full-splitter" />
<div id="listContainer"></div>