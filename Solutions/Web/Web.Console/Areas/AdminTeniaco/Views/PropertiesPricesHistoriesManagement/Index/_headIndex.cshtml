@using VM.Teniaco;
@using Newtonsoft.Json;

<script type="text/javascript">
    @{
        PropertiesVM property = new PropertiesVM();
        @Html.Raw("var property;");

        if (ViewData["Properties"] != null)
        {
            property = (PropertiesVM)ViewData["Properties"];

            @Html.Raw("debugger; property =" +JsonConvert.DeserializeObject(JsonConvert.SerializeObject(property))+" ;debugger;")
            ;
        }
    }

</script>




