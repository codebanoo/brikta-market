    @await RenderSectionAsync("Scripts", required: false)
    <script>

        var showTimer;
        var tempVerify;
        $(document).ready(function () {

            $("#melkavan-icon").click(function (e) {
                e.preventDefault();
                //e.stopProg
                $.removeCookie('OnlyFavorite', { path: '/' });
                $.removeCookie('searchString', { path: '/' });
                //window.location.href = '/UserMelkavan/MelkavanPanel/Index';
                window.location.href = '/';
            });

            $("#chat-icon").click(function (e) {
                e.preventDefault();
                $('#alertText').text('هیچ پیام جدیدی ندارید!');
                $('#alertModal').modal('show');
            });

            $('#resendLink').click(function (e) {
                e.preventDefault();
                sendVerificationCode();
            });

            $("#sendVerificationCode").click(function (e) {
                e.preventDefault();
                sendVerificationCode();
            });

            $('#confirmCode').click(function (event) {
                event.preventDefault();
                $('#verifyModal').modal('hide');
                $('#loadingModal').modal('show');
                var phone = $('#verifyUserName').val();
                var captchaCode = tempVerify;
                var num1 = $('#num_1').val();
                var num2 = $('#num_2').val();
                var num3 = $('#num_3').val();
                var num4 = $('#num_4').val();
                var confirmCode = num1 + num2 + num3 + num4;

                debugger;
                $.ajax({
                    cache: false,
                    type: "Post",
                    url: "/UserMelkavan/MelkavanPanel/MelkavanVerify",
                    data: { 'UserName': phone, 'Password': confirmCode, 'RememberMe': false, 'CaptchaCode': captchaCode },
