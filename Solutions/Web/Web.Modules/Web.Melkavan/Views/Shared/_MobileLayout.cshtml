                            window.setTimeout(function () { $('#loadingModal').modal('hide'); }, 1000);
                            clearInterval(showTimer);

                        }
                        //debugger;

                        //$('#verifyModal').modal('hide');
                        //var $url = '/UserMelkavan/MelkavanPanel/Index';
                    },
