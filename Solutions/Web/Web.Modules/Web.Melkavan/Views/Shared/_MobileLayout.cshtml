            $('body').on('keyup', '.activation-code-input', function (e) {
                debugger;
                e.preventDefault();

                var id = parseInt($(this).attr('id').split('_')[1]);
                if ($(this).val() != "") {
                    $('#num_' + (id + 1)).focus();
                }
            });

            $('body').on('click', '#backButton', function (e) {
                e.preventDefault();
                history.back();
            });

            $('#verifyModal').on('shown.bs.modal', function (e) {
                $("#num_1").val("");
                $("#num_2").val("");
                $("#num_3").val("");
                $("#num_4").val("");
            })

            function sendVerificationCode() {
                debugger;
                var phone = $('#mobileNumber').val();
                if (phone != '') {
                    debugger;
                    if ((phone.length < 11) || (!phone.startsWith('09'))) {
                        $('#alertText').text('شماره موبایل را صحیح وارد کنید');
                        $('#alertModal').modal('show');
                        return;
                    }

                    $('#loginModal').modal('hide');
                    $('#loginModal').removeClass('show');
                    $('#loginModal').hide();
                    $('#loadingModal').modal('show');

                    phone = phone.replace(/[۰-۹]/g, d => '۰۱۲۳۴۵۶۷۸۹'.indexOf(d));

                    $('.timer').show();
                    $('.resendCodeBlock').hide();

                    var start = 120;
                    showTimer = setInterval(function () {

                        if (start != 0) {
                            $('.timer').text(start + " ثانیه");
                            start--;
                        }
                        else {
                            $('.timer').hide();
                            $('.resendCodeBlock').show();
                            clearInterval(showTimer);
                        }

                    }, 1000);

                    $.ajax({
                        cache: false,
                        type: "Post",
                        url: "/UserMelkavan/MelkavanPanel/MelkavanLogin",
                        data: { 'UserName': phone, 'Password': "pass", 'RememberMe': false, 'CaptchaCode': "" },
                        success: function (response) {

                            const showverifyModal = setInterval(() => {
                                $('#loadingModal').modal('hide');
                                $('#loadingModal').hide();
                                $('#loadingModal').removeClass('show');
                                $('#verifyModal').modal('show');

                                clearInterval(showverifyModal);
                            }, "1000");
                            debugger;
                            $('#verifyUserName').val(response.userName);
                            tempVerify = response.verify;
                            //$('#verifyUserName').val(response.userName);
                        },
                        error: function () {
                            $('#loadingModal').modal('hide');
                            $('#loadingModal').hide();
                            $('#loadingModal').removeClass('show');
                            $('#alertText').text('خطا');
                            $('#alertModal').modal('show');
                            clearInterval(showTimer);
                        }
                    });
                }
                else {
                    $('#alertText').text('شماره موبایل را وارد کنید');
                    $('#alertModal').modal('show');
                }
            }
        });
    </script>



